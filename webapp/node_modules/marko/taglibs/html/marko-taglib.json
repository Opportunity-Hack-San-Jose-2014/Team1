{
    "tags": {
        "html": {
            "attributes": {
                "html-doctype": {
                    "type": "string"
                }
            }
        },
        "html-doctype": {
            "attributes": {
                "value": {
                    "type": "custom"
                }
            },
            "node-class": "./DocTypeNode"
        },
        "*": {
            "transformer": "./html-tag-transformer"
        },
        "html-comment": {
            "renderer": "./CommentTag"
        }
    }
}